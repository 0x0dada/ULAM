##
## informed by t41025: gcc permits default case anywhere, but only hit if nothing else does
##
#>A.ulam
  ulam 2;
element A {
  System s;
  Int a, b;
  Int test() {
    a = 1;
    b = 0;
    switch((--a))
    {
    default: //first in line, last to hit
      {
	b = 1;
      }
    case 0:
      {
	b = 2; //hits, no fall through
      }
    case -1:
      {
	b = 3;
      }
    };
    s.print(b);
    s.assert(b == 2);
    s.print(a);
    s.assert(a == 0);
    return a;
  }
}

#:System.ulam
ulam 1;
