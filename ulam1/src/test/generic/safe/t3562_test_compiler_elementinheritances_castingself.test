##
## chained class inheritance, regular (non-template), with casting self
## informed by t3559, t3229
##
#>Foo.ulam
  ulam 1;
use System;
use Soo;
element Foo : Soo {
  Bool(7) sp;
  Int a = 73;
  Void setus(Unsigned(3) x)
  {
    us = x;
  }

  Int test() {
    System s;
    setus(3u);
    Coo cc = (Coo) self; //also works
    s.print(cc.us); //default 2u
    cc.setus(7u);

    if(cc.cb)
      {
	s.print(us); //3u
	s.print(cc.us); //7u
	return us + cc.us; //exit status 10
      }
    return 0;
  }
}

