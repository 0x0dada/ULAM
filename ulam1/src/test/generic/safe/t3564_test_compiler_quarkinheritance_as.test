##
## quark inheritance, regular (non-template) with casting
## informed by t3560, t3536, t3559
##
#>Foo.ulam
  ulam 1;
use Soo;
use System;
element Foo {
  System s;
  Bool first;
  Soo su;
  Int test() {
    Foo f;
    //'has' works like 'as'
    if(f has Coo)
    {
      first = f.cb;
      s.print(first);
      s.print(f.sizeof);
      return f.sizeof;
    }
    return 0;
  }
}

#:Soo.ulam
  ulam 1;
use Coo;
quark Soo : Coo {
  Unsigned(3) us = 4u;
}

#:Coo.ulam
  ulam 1;
quark Coo {
  Bool(3) cb = true;
}

#:System.ulam
ulam 1;
quark System {
Void print(Unsigned arg) native;
Void print(Int arg) native;
Void print(Int(4) arg) native;
Void print(Int(3) arg) native;
Void print(Unary(3) arg) native;
Void print(Bool(3) arg) native;
Void assert(Bool b) native;
}

