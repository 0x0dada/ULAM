use System;
use QW;
element A : QW {
  Bool b, c;
  Int test(){
    System s;
    Qbase& qref = self;
    qref.m_five = 0x3;
    A apple = (A) qref;
    b = (apple.m_five == 0x3);
    s.assert(b);
    return (Int) b;
  }
}

