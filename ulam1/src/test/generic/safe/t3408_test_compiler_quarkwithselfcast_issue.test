  ulam 1;
use IntXY;
use System;
element Foo : IntXY(3u,4u){
  Int test() {
    System s;
    Foo m, n;
    s.print(m.x);
    s.print(m.y);
    s.print(n.x);
    s.print(n.y);

    m = make(1,2); //works!!
    s.print(m.x);
    s.print(m.y);
    s.print(n.x);
    s.print(n.y);

    m = n.makeAs(1,2);
    //m.init(1,2); //works
    s.print(m.x);
    s.print(m.y);
    s.print(n.x);
    s.print(n.y);

    m = (Foo) fromXY(2,3);
    s.print(m.x);
    s.print(m.y);

    init(2,2);
    s.print(self.x); //unresolved wo self
    s.print(self.y); //unresolved wo self

    return 0;
  }

  Self makeAs(Int x, Int y) {
    Atom a = self;
    if(a as Self){
      a.init(x,y);
    }
    if(a is Self)
    {
      self = (Self) a;
      return self;
    }
    return (Self) a;
  }

  Self make(Int x, Int y) {
    Self ret;
    ret.init(x,y);
    return ret;
   }

}

#:IntXY.ulam
  ulam 1;
quark IntXY(Unsigned xbits, Unsigned ybits) {
  typedef IntXY(xbits, ybits) Self;
  typedef Int(xbits) XCoord;
  typedef Int(ybits) YCoord;
  XCoord x;
  YCoord y;
  Atom fromXY(Int x, Int y) {
    init(x,y);
    return self;
  }

  Void init(Int ax, Int ay) {
    x = (XCoord) ax;
    y = (YCoord) ay;
    if(x == x.minof) ++x;
    if(y == y.minof) ++y;
  }
}
