## see t3588 and t3589 for 'as' and 'is' versions
##
#:Typo.ulam
  ulam 1;
use Bar;
quark Typo(Int a) : Bar{
  Bool b; //shadows for no reason
}

#:Bar.ulam
  ulam 1;
quark Bar
{
  Bool b; //testing offset
  Unsigned(3) bu = 7u;
  Void func(Unsigned u)
  {
    bu = (Unsigned(3)) u;
  }
}

#>Eltypo.ulam
  ulam 1;
use Typo;
use System;
element Eltypo {
  System s;
  Unsigned(3) first;
  Unsigned(3) second;
  Typo(1) t;
  Int test(){
    Eltypo elf;
    if(elf has Bar){
      elf.func(3u);
      s.print(elf.bu);
      first = elf.bu; //3u, from 7u
    }
    s.print(elf.t.bu);
    s.print(first);
    second = elf.t.bu; //3u, from 7u (no longer fails eval!!).
    return second;
  }
}

#:System.ulam
ulam 1;
quark System {
Void print(Unsigned arg) native;
Void print(Int arg) native;
Void print(Int(4) arg) native;
Void print(Int(3) arg) native;
Void print(Unary(3) arg) native;
Void print(Bool(3) arg) native;
Void assert(Bool b) native;
}
