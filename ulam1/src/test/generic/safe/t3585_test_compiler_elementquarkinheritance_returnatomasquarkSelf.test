##
## element-quark inheritance, regular (non-template) with casting
## informed by t3564, t3560, t3536, t3559
##
#>Foo.ulam
  ulam 1;
use Soo;
use System;
element Foo : Soo {
  Bool first;
  Unsigned(3) cus = 6u;
  Unsigned(3) dus = 6u;
  Int test() {
    System s;
    Coo c = func();
    cus = c.us;
    s.print(c.us);

    Coo d = c.func(); //what happens when atom is not an Atom, immediate UNDEFINED_TYPE
    dus = d.us;
    s.print(d.us);

    s.print(c.us + d.us); //6
    s.print(cus + dus); //4
    return (c.us + d.us);
  }
}

#:Soo.ulam
  ulam 1;
use Coo;
quark Soo : Coo {
  Unsigned(3) us = 4u;
}

#:Coo.ulam
  ulam 1;
quark Coo {
  Unsigned(3) us = 2u;
  Bool(3) cb = true;
  Self func()
  {

    Atom f = atom; //was self
    //Foo f = (Foo) self;
    if(f as Foo) //for eval
    {
      if(f as Coo)
      {
	f.us = 1u;
	return f;
      }
    }
    us = 3u;
    return self;
  }
}

#:System.ulam
ulam 1;
quark System {
Void print(Unsigned arg) native;
Void print(Int arg) native;
Void print(Int(4) arg) native;
Void print(Int(3) arg) native;
Void print(Unary(3) arg) native;
Void print(Bool(3) arg) native;
Void assert(Bool b) native;
}

