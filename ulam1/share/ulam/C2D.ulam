ulam 1;
quark C2D {
  typedef Int(16) Coord;
  Coord x;
  Coord y;

  C2D make(Int x, Int y) {
    C2D ret;
    ret.x = x;
    ret.y = y;
    return ret;
  }
  Unsigned manhattanDistance(C2D other) {
    Int dx = x - other.x, dy = y - other.y;
    if (dx < 0) dx = -dx;
    if (dy < 0) dy = -dy;
    return (Unsigned) (dx + dy);
  }
}
